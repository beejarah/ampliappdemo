{
  "name": "amplidemo",
  "main": "entrypoint.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --port 8082",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android --port 8082",
    "ios": "expo start --ios --port 8082",
    "web": "expo start --web --port 8082",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "kill": "kill-port 8081 8082",
    "fresh-start": "kill-port 8081 8082 && expo start --port 8082",
    "demo": "kill-port 8081 8082 && npx expo start --port 8082",
    "build": "expo export --platform web",
    "vercel-build": "expo export --platform web",
    "start:8082": "npx expo start --port 8082"
  },
  "jest": {
    "preset": "jest-expo"
  },
